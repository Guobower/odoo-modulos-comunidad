<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
	<!-- add date_expected on move line in delivery order view -->
        <record id="view_picking_out_form_add_expected_date_move" model="ir.ui.view">
            <field name="name">stock.picking.out.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_out_form"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <xpath expr="//tree//field[@name='date']" position="after">
                   <field name="date_expected" />
            </xpath>
            </field>
        </record>

    <!-- add date_done on view delivery order tree --> 
        <record id="view_picking_out_tree_add_expected_date_move" model="ir.ui.view">
            <field name="name">stock.picking.out.tree</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_out_tree"/>
            <field name="type">tree</field>
            <field name="arch" type="xml">
            <xpath expr="//tree//field[@name='min_date']" position="after">
                   <field name="date_done" />
            </xpath>
            </field>
        </record>

    <!-- add date_done on view delivery order tree --> 
        <record id="view_picking_tree_add_expected_date_move" model="ir.ui.view">
            <field name="name">stock.picking.tree</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="type">tree</field>
            <field name="arch" type="xml">
            <xpath expr="//tree//field[@name='min_date']" position="after">
                   <field name="date_done" />
            </xpath>
            </field>
        </record>



        <record id="view_move_tree_reception_picking_add_expected_date_move" model="ir.ui.view">
            <field name="name">stock.move.tree2</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_tree_reception_picking"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <xpath expr="//tree//field[@name='date']" position="after">
                   <field name="date_expected" />
            </xpath>
            </field>
        </record>


    </data>
</openerp>
